import(io="io", ds="ds", rd="random", nr="newrcc")

var List: clas = ds.List

fun makeRandomIntList(): List {
    var li: List = List()
    ds.repeat(10, (_) -> {
        li.append(rd.randomInt(0, 100))
    })
    ret li
}

fun main(): void {
    var li = List()
    for ({var i = 0; i < 10; i ++}) {
        li.append(makeRandomIntList())
    }

    io.sout(nr.ctext("li: ", nr.TextColor.BLUE), li.map((item: List): list -> {
        ret item.get()
    }).toStr())

    var maxList: List = li.max((a: List, b: List): int -> {
        ret a.sum() - b.sum()
    })

    io.sout(nr.ctext("max of li: ", nr.TextColor.YELLOW), maxList.get(),
        nr.ctext(" sum of this: ", nr.TextColor.CYAN), maxList.sum())
    io.sout(nr.ctext("max of max of li: ", nr.TextColor.RED), maxList.max())
}

main()
