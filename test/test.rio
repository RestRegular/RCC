import(io="io", ll="lowlevel")

class Error {
    ctor () {}
    fun toString(): str public virtual {
        ret "[Error]"
    }
}

class FileError: Error {
    ctor () {
        super()
    }
    fun toString(): str public overwrite {
        ret "[FileError]"
    }
}

try {
    var error: Error = Error()
    io.sout("Running try block, expose an error: ", error.toString())
    throw error
} catch (error: FileError) {
    io.sout("Running catch block, caught an error: ", error.toString())
} finally {
    io.sout("Running finally block")
}
